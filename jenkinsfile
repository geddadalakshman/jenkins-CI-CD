pipeline {
    agent { label 'terraform' }
    parameters {
            string(name: 'PERSON', defaultValue: 'Mr Jenkins', description: 'Who should I say hello to?')
        }

    stages {
        stage (one){
            steps{
                echo "one"
            }

        }
        stage (two){
            steps{
                echo "two"
            }
        }
    }
}

