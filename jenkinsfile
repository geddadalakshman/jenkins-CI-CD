pipeline {
    agent { label 'terraform' }
    environment{
        SSH = credentials("SSH")
    }
    stages {
        stage ('one'){
            steps{
                echo "one"
                sh "env"
            }
        }
        stage ('two'){
            steps{
                echo "two"
            }
        }
    }
    post {
        failure {
            echo 'I will always say Hello again!'
        }
    }
}
