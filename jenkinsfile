pipeline {
    agent { label 'terraform' }
    stages {
        stage (one){
            input {
                message "Should we continue?"
                ok "Yes, we should."
                submitter "alice,bob"
                parameters {
                    string(name: 'PERSON', defaultValue: 'Mr Jenkins', description: 'Who should I say hello to?')
                }
            steps{
                echo "one"
            }
        }
        stage (two){
            steps{
                echo "two"
            }
        }
    }
}
